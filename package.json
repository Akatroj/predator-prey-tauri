    "@pixi/events": "^7.2.3",
    "@pixi/extensions": "^7.2.3",
    "@pixi/graphics": "^7.2.3",
    "@pixi/math": "^7.2.3",
    "@pixi/mesh": "^7.2.3",
    "@pixi/mesh-extras": "^7.2.3",
    "@pixi/particle-container": "^7.2.3",
    "@pixi/sprite": "^7.2.3",
    "@pixi/sprite-animated": "^7.2.3",
    "@pixi/sprite-tiling": "^7.2.3",
    "@pixi/text": "^7.2.3",
