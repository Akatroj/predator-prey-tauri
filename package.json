{
  "name": "predator-prey",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri-dev": "tauri dev",
    "start": "concurrently npm:watch npm:tauri-dev --kill-others",
    "watch": "npm-watch",
    "zip-python": "cd src-python/src && bestzip ../../public/python.zip *",
    "lint": "eslint src/",
    "fix": "eslint src/ --fix && prettier --write src/"
  },
  "watch": {
    "zip-python": {
      "patterns": [
        "src-python/src"
      ],
      "extensions": "py"
    }
  },
